   "source": [
    "#from sqlalchemy import create_engine\n",
    "\n",
    "# 創建與SQLite資料庫的連接\n",
    "engine = create_engine('sqlite:///stock_data.db')\n",
    "\n",
    "# 定義要刪除的表名\n",
    "table_name = 'stock_data_2022'\n",
    "\n",
    "# 執行SQL命令來刪除表\n",
    "with engine.connect() as con:\n",
    "    con.execute(f\"DROP TABLE IF EXISTS {table_name}\")"
   ]
  }
